{
  "name": "unsplash-gallery",
  "private": true,
  "version": "1.0.0",
  "description": "This is a fully working example of Next.js 9.3 running on [Preact](https://github.com/preactjs/preact) instead of React.",
  "keywords": [],
  "author": "vinaypuppal",
  "license": "UNLICENSE",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-on-netlify && cp _headers out_publish/",
    "start": "NODE_ENV=production next start",
    "format": "prettier --write './**/*.+(js|jsx|json|md|tsx|ts|gql|css)'"
  },
  "devDependencies": {
    "@prefresh/next": "^1.1.0",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "next-on-netlify": "^2.4.0",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "react-refresh": "^0.8.3",
    "tailwindcss": "^1.8.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@reach/combobox": "^0.11.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-portal": "^4.0.2",
    "@types/unsplash-js": "^6.0.1",
    "clsx": "^1.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.0",
    "next-seo": "^4.7.3",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.4",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-portal": "^4.2.1",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.0.1",
    "swr": "^0.3.2",
    "tinykeys": "^1.1.0",
    "unsplash-js": "^6.3.0",
    "use-onclickoutside": "^0.3.1"
  },
  "lint-staged": {
    "*.{js|jsx|tsx|ts|gql|css|md|json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
